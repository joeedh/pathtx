{
  "compilerOptions": {
    "allowJs": true,
    "target": "es6",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "lib": [
      "dom",
      "scripthost",
      "es2022"
    ],
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "pretty": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "strictNullChecks": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "noEmitOnError": false,
    "declaration": false,
    "noEmit": true,
    "noImplicitThis": true,
    "listEmittedFiles": false,
    "paths": {
    }
  },
  "files": [
    "core/index.ts",
    "util/index.ts",
    "core/units/index.ts"
  ],
}


